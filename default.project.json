{
	"name": "roblox-ts-game",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"game": {
				"$path": "out/server"
			}
		},
		"Players": {
			"$className": "Players",
			"$properties": {
				"CharacterAutoLoads": false
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"@flamework": {
						"$path": "node_modules/@flamework"
					},
					"@memolemo-studios": {
						"$path": "node_modules/@memolemo-studios"
					},
					"$path": "node_modules/@rbxts"
				}
			},
			"game": {
				"$path": "out/shared"
			},
			"lualib": {
				"$path": "lib"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"game": {
					"$path": "out/client"
				}
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true
			}
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		}
	}
}
